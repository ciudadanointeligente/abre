<!-- Hero -->
<div class="no-margin-top">
  <div class="hero">
    <div class="panel-pane pane-fieldable-panels-pane pane-current-1 pane-bundle-complex-block pane--hero-block hiveboxx-waypoints-processed hiveboxx-cta-processed pane--waypoint" id="pane-id-1">
      <div class="pane-content">
        <div class="ds-1col-stacked fieldable-panels-pane view-mode-full clearfix">
          <div class="group-header">
            <div class="field field--name-field-background-image field--type-image field--label-hidden"><div class="field__items"><div class="field__item even"><%= image_tag(image_path_for('hero01.png')) %></div></div></div>
          </div>
          <div class="group-ds_content">
            <div class="field field--name-field-pre-title field--type-text field--label-hidden row">
              <div class="field__items large-8 large-offset-2 columns">
                <div class="field__item even">
                  <h1 class="white text-center"> La comunidad decide el futuro de sus barrios </h1>
                </div>
                <h5 class="white text-center"> La Municipalidad de Peñalolén sabe que eres tú quien mejor conoce la comuna y sabe lo que necesita, por eso te invitamos a proponer, diseñar y construir en conjunto con los funcionarios y funcionarias municipales una mejor comuna. </h5>
                <div class="text-center">
                  <%= link_to t('Mira los desafíos'), problems_path, class: "button primary large" %>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="row">
          <div class="large-3 columns hero-about">
            <div class="small white">
              Qué es abre
            </div>
            <p class="white">Abre es una mezcla de herramientas digitales y metodológicas que acercan el trabajo de municipios a vecinas y vecinos, potenciando la construcción colectiva de barrios y comunidades.</p>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<main>
<!-- Desafíos -->
<section id="problems-in-home">
  <div class="text-width">
    <h2 class="text-center">Desafíos que ofrece la municipalidad</h2>
    <div class="row">
        <% Problem.first(2).each do |problem| %>
        <div class="large-6 columns">
        <div class="problem-box">
          <div>
            <div class="small-without-line blue">Etapa: <%= problem.status %></div>
            <h5><%= problem.title %></h5>
            <p class="details"><i class="fa fa-map-marker" aria-hidden="true"></i> <%= problem.geozones_name %> <span class="line-text"> | </span>
            <i class="fa fa-calendar" aria-hidden="true"></i> <%= l problem.created_at.to_date %></span></p>
            <p><%= problem.summary.truncate(130, :omission => "...") %><%= link_to ' Ver más', proposals_path(:challenge=>problem.id) %></p>
          </div>
          <div class="fixed-bottom">
          <!-- Para etapa propuestas -->
          <% if problem.status=='Propuestas' %>
          <div class="bg-blue padding-small white text-center">
              <% if problem.valid %>
                <%= t("proposals.index.remaining_time_1") %> <span class="counter"> <%= distance_of_time_in_days(problem.ends_at, Date.today) %></span><%= t("proposals.index.remaining_time_2") %>
              <% else %>
                Proceso de recepción de propuestas cerrado
              <% end %>
            </div>
          <!-- Para etapa diseño -->
          <% elsif problem.status=='Diseño' %>
          <!-- TODO: agregar funcion que dibuje la fecha del proximo taller -->
            <div class="bg-white text-center padding-small">
              Próximo taller de diseño: <span class="counter">03/11/17</span>
            </div>
          <% end %>
            <div class="padding text-right">
              <!-- Para etapa propuestas -->
              <% if problem.status=='Propuestas' %>
              <%= link_to t("Ver desafío"), proposals_path(:challenge => problem.id), class: 'button tiny secondary' %>
                <% if problem.valid %>
                  <%= link_to t("proposals.index.start_proposal"), new_proposal_path(:problem => problem.id), class: 'button tiny' %>
                <% end %>
              <!-- Para etapa talleres -->
              <% elsif problem.status=='Diseño' %>
                <%= link_to t("Ver información del taller"), project_design_events_path(:project_id => problem.project.id ,:problem_id => problem.id), class: 'button tiny' %>
              <!-- Para etapa implementacion -->
              <% elsif problem.status=='Implementación' %>
                <%= link_to t("Hacer seguimiento"), project_reports_path(:project_id => problem.project.id ,:problem_id => problem.id), class: 'button tiny' %>
              <!-- Para etapa evaluacion -->
              <% elsif problem.status=='Evaluación' %>
                <%= link_to t("Evaluar el proceso"), project_evaluations_path(:project_id => problem.project.id ,:problem_id => problem.id), class: 'button tiny' %>
              <% end %>
            </div>
          </div>
        </div>
      </div>
          <% end %>
    </div>
  </div>
</section>



